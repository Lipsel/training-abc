<odoo>
    <record id="view_real_estate_tag_tree" model="ir.ui.view">
      <field name="name">real.estate.tag.tree</field>
      <field name="model">real.estate.tag</field>
      <field name="arch" type="xml">
        <list>
          <field name="name"/>
      </list>
      </field>
    </record>
  
    <record id="view_real_estate_tag_form" model="ir.ui.view">
      <field name="name">real.estate.tag.form</field>
      <field name="model">real.estate.tag</field>
      <field name="arch" type="xml">
        <form>
          <sheet>
            <group>
              <field name="name"/>
            </group>
          </sheet>
        </form>
      </field>
    </record>
  
    <record id="action_real_estate_tag" model="ir.actions.act_window">
      <field name="name">Tags</field>
      <field name="res_model">real.estate.tag</field>
      <field name="view_mode">list,form</field>
    </record>
  
    <menuitem id="menu_real_estate_tag"
              name="Tags"
              parent="real_estate_menu_root"
              action="action_real_estate_tag"/>
  </odoo>
  